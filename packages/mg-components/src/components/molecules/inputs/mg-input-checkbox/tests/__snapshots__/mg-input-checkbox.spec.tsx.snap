                    <slot name="append-input"></slot>
                    <mg-input-title identifier="identifier-input-search" is-legend="false" readonly="false" required="false" slot="label">
                      <!---->
                      <label class="mg-c-input-title" htmlfor="identifier-input-search">
                        Enter a value
