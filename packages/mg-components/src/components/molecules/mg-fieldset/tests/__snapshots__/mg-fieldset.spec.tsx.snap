    <mg-input aria-labelledby="identifier-title" class="mg-c-input mg-c-input--fieldset mg-c-input--has-error mg-c-input--label-on-top" role="group">
      <template shadowrootmode="open">
        <div class="mg-c-input__title">
          <slot name="label"></slot>
        </div>
        <div class="mg-c-input__input-container">
          <div class="mg-c-input__input">
            <slot></slot>
          </div>
          <div aria-live="assertive" class="mg-c-input__error">
            <slot name="error"></slot>
          </div>
        </div>
      </template>
      <span class="mg-c-input__input-group">
        <slot></slot>
      </span>
      <div id="identifier-error" slot="error">
        this is a custom error
      </div>
      <mg-input-title identifier="identifier" is-legend="true" readonly="false" required="false" slot="label">
        <!---->
        <legend aria-describedby="identifier-error" class="mg-c-input-title" id="identifier-title">
          legend
        </legend>
      </mg-input-title>
    </mg-input>
  </template>
  <mg-input-radio>
    <template shadowrootmode="open">
      <mg-input aria-labelledby="mg-input-radio-title" class="mg-c-input mg-c-input--fieldset mg-c-input--radio" role="group">
        <template shadowrootmode="open">
          <div class="mg-c-input__title">
            <slot name="label"></slot>
          </div>
          <div class="mg-c-input__input-container">
            <div class="mg-c-input__input">
              <slot></slot>
            </div>
          </div>
        </template>
        <ul class="mg-c-input__input-group-container" role="list">
          <li class="mg-c-input__input-group">
            <input id="mg-input-radio_0" name="mg-input-radio" type="radio" value="0">
            <label htmlfor="mg-input-radio_0">
              batman
            </label>
          </li>
          <li class="mg-c-input__input-group">
            <input id="mg-input-radio_1" name="mg-input-radio" type="radio" value="1">
            <label htmlfor="mg-input-radio_1">
              robin
            </label>
          </li>
          <li class="mg-c-input__input-group">
            <input id="mg-input-radio_2" name="mg-input-radio" type="radio" value="2">
            <label htmlfor="mg-input-radio_2">
              joker
            </label>
          </li>
          <li class="mg-c-input__input-group">
            <input id="mg-input-radio_3" name="mg-input-radio" type="radio" value="3">
            <label htmlfor="mg-input-radio_3">
              bane
            </label>
          </li>
        </ul>
        <mg-input-title identifier="mg-input-radio" is-legend="true" readonly="false" required="false" slot="label">
          <!---->
          <legend class="mg-c-input-title" id="mg-input-radio-title">
            mg-input-radio label
          </legend>
        </mg-input-title>
      </mg-input>
    </template>
  </mg-input-radio>
  <mg-input-text>
    <template shadowrootmode="open">
      <mg-input class="mg-c-input mg-c-input--text mg-c-input--width-full">
        <template shadowrootmode="open">
          <div class="mg-c-input__title">
            <slot name="label"></slot>
          </div>
          <div class="mg-c-input__input-container">
            <div class="mg-c-input__input">
              <slot></slot>
            </div>
          </div>
        </template>
        <div class="mg-c-input__with-character-left" style="--mg-c-character-left-message-length: 7;">
          <input aria-describedby="mg-input-text-character-left" aria-invalid="false" class="mg-c-input__box mg-c-input__box--width" id="mg-input-text" maxlength="400" name="mg-input-text" type="text">
          <mg-character-left identifier="mg-input-text-character-left" maxlength="400"></mg-character-left>
        </div>
        <slot name="append-input"></slot>
        <mg-input-title identifier="mg-input-text" is-legend="false" readonly="false" required="false" slot="label">
          <!---->
          <label class="mg-c-input-title" htmlfor="mg-input-text">
            mg-input-text label
          </label>
        </mg-input-title>
      </mg-input>
    </template>
  </mg-input-text>
</mg-fieldset>
`;

