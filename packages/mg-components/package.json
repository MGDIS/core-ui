{
  "name": "@mgdis/mg-components",
  "version": "5.10.1",
  "description": "MG Components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mg-components/mg-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "maintainers": [
    "Simon Duhem <duhem-s@mgdis.fr>",
    "Nicolas Jouanno <jouanno-n@mgdis.fr>"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "lint": "run-s lint:es lint:prettier",
    "lint:fix": "run-s lint:es:fix lint:prettier:fix",
    "lint:es": "eslint src/**/*.{ts,tsx}",
    "lint:es:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:prettier": "prettier --check **/*.{ts,tsx,scss,html,mdx,json}",
    "lint:prettier:fix": "prettier --write **/*.{ts,tsx,scss,html,mdx,json}",
    "test": "TZ=UTC stencil test --spec --e2e",
    "test:unit": "TZ=UTC stencil test --spec --collectCoverage",
    "test:e2e": "stencil test --e2e",
    "test:e2e:playwright": "pnpm --package=@pnpm/@playwright/test dlx playwright test",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "storybook:build": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "coverage": "TZ=UTC stencil test --spec --no-build --collectCoverage --reporters=\"default\" --reporters=\"jest-junit\""
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "@stencil/core": "^2.22.1",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@mgdis/linting-stencil": "workspace:*",
    "@playwright/test": "^1.34.3",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jest": "npm:@types/jest@^26",
    "@types/jest-image-snapshot": "^4.3.2",
    "@types/node": "^18.14.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "chromatic": "^6.15.0",
    "css-loader": "^5.2.7",
    "eslint": "^8.39.0",
    "eslint-config-stencil": "workspace:*",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "npm:jest@^26",
    "jest-cli": "npm:jest-cli@^26",
    "jest-image-snapshot": "^4.5.1",
    "jest-junit": "^15.0.0",
    "jsx-dom": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "playwright-config": "workspace:^1.0.0",
    "prettier": "^2.8.4",
    "puppeteer": "^13.7.0",
    "sass": "^1.58.0",
    "sass-loader": "^10.4.1",
    "style-loader": "^2.0.0",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5"
  },
  "license": "BSD-3-Clause"
}
