{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 100
        }
      }
    ],
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "pragma": "h"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-jsx-pragmatic",
      {
        "export": "h",
        "import": "h",
        "module": "jsx-dom"
      }
    ]
  ]
}
