{
  "name": "vue-2",
  "private": true,
  "version": "0.0.26",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:es && pnpm lint:prettier",
    "lint:fix": "pnpm lint:es:fix && pnpm lint:prettier:fix",
    "lint:es": "eslint 'src/**/*.{vue,js,jsx}' --ignore-path ../../.gitignore",
    "lint:es:fix": "eslint 'src/**/*.{vue,js,jsx}' --fix --ignore-path ../../.gitignore",
    "lint:prettier": "prettier --check '**/*.{scss,vue,js,jsx,md,json}' --ignore-path ../../.gitignore",
    "lint:prettier:fix": "prettier --write '**/*.{scss,vue,js,jsx,md,json}' --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@mgdis/mg-components": "workspace:*",
    "@mgdis/notification-center": "workspace:*",
    "@mgdis/styles": "workspace:*",
    "vue": "npm:vue@2.7.16"
  },
  "devDependencies": {
    "@mgdis/eslint-plugin-vue": "workspace:*",
    "@mgdis/linting-vue2": "workspace:*",
    "sass": "1.77.8",
    "vite": "5.3.5",
    "vite-plugin-vue2": "2.0.3"
  }
}
