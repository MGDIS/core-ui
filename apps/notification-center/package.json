{
  "name": "notification-center",
  "private": true,
  "version": "1.0.13",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3210",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-s lint:es lint:prettier",
    "lint:fix": "run-s lint:es:fix lint:prettier:fix",
    "lint:es": "eslint src/**/*.{ts,tsx}",
    "lint:es:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:prettier": "prettier --check **/*.{js,ts,tsx,html,json} --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write **/*.{js,ts,tsx,html,json} --ignore-path .gitignore"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier-config": "workspace:*",
    "typescript": "^4.9.5",
    "vite": "^4.3.2"
  },
  "dependencies": {
    "@mgdis/mg-components": "workspace:*",
    "@mgdis/notification-center": "workspace:*"
  }
}
