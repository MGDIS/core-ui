{
  "name": "angularjs",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:es && pnpm lint:prettier",
    "lint:fix": "pnpm lint:es:fix && pnpm lint:prettier:fix",
    "lint:es": "eslint 'src/**/*.{html,js}' --ignore-path ../../.gitignore",
    "lint:es:fix": "eslint 'src/**/*.{html,js}' --fix --ignore-path ../../.gitignore",
    "lint:prettier": "prettier --check '**/*.{scss,html,md,json}' --ignore-path ../../.gitignore",
    "lint:prettier:fix": "prettier --write '**/*.{scss,html,md,json}' --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@mgdis/mg-components": "workspace:*",
    "@mgdis/mg-components-helpers": "workspace:*",
    "@mgdis/notification-center": "workspace:*",
    "@mgdis/styles": "workspace:*",
    "angular": "1.8.3"
  },
  "devDependencies": {
    "@html-eslint/eslint-plugin": "0.27.0",
    "@html-eslint/parser": "0.27.0",
    "@mgdis/eslint-plugin-html": "workspace:*",
    "@mgdis/linting-angularjs": "workspace:*",
    "sass": "1.80.6",
    "vite": "5.4.10"
  }
}
