{
  "name": "vue-3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "app": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build-only": "vite build",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "lint": "run-s lint:es lint:prettier",
    "lint:fix": "run-s lint:es:fix lint:prettier:fix",
    "lint:es": "eslint src/**/*.{vue,ts,tsx} --ignore-path .gitignore",
    "lint:es:fix": "eslint src/**/*.{vue,ts,tsx} --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --check **/*.{scss,vue,ts,tsx,md,json}",
    "lint:prettier:fix": "prettier --write **/*.{scss,vue,ts,tsx,md,json}"
  },
  "dependencies": {
    "@mgdis/mg-components": "workspace:*",
    "@mgdis/notification-center": "workspace:*",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@mgdis/eslint-config-vue3": "workspace:*",
    "@rushstack/eslint-patch": "^1.x",
    "@tsconfig/node18": "^1.0.1",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint-plugin-vue": "^9.x",
    "jsdom": "^22.1.0",
    "sass": "^1.58.0",
    "typescript": "^4.9.5",
    "vite": "4",
    "vue-tsc": "^1.8.11"
  }
}
