{
  "name": "eslint-config-stencil",
  "version": "1.1.1",
  "description": "",
  "main": "./src/index.js",
  "private": true,
  "scripts": {
    "lint": "pnpm lint:es && pnpm lint:prettier",
    "lint:fix": "pnpm lint:es:fix && pnpm lint:prettier:fix",
    "lint:es": "eslint 'src/**/*.js' --ignore-path ../../.gitignore",
    "lint:es:fix": "eslint 'src/**/*.js' --fix --ignore-path ../../.gitignore",
    "lint:prettier": "prettier --check '**/*.{js,json}' --ignore-path ../../.gitignore",
    "lint:prettier:fix": "prettier --write '**/*.{js,json}' --ignore-path ../../.gitignore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stencil-community/eslint-plugin": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.x",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-jsx-a11y": "^6.7.1"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.32.2",
    "prettier-config": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.0"
  }
}
